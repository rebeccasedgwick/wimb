<h1>Create Court</h1>
<%= form_with scope: :court, url: courts_path, local: true do |form| %>
  <p>
    <%= form.label :name %> <br>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :staff_total %> <br>
    <%= form.number_field :staff_total %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>
